---
version: '3'

# This networks section required for the package to be visible to Traefik.
# DO NOT Remove
networks:
  traefik_network:
    external:
      name: homelabos_traefik

services:
  app: 
    image: 42wim/matterbridge
    restart: unless-stopped
    volumes: {{ volumes_root }}/matterbridge/matterbridge.toml:/matterbridge.toml
    networks:
      - traefik_network
    
